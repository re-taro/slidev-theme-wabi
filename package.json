{
  "name": "slidev-theme-wabi",
  "description": "Japanese wabi theme for slidev",
  "version": "1.0.0",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "license": "MIT",
  "author": {
    "name": "Rintaro Itokawa",
    "email": "me@re-taro.dev",
    "url": "https://re-taro.dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/re-taro/slidev-theme-wabi"
  },
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png",
    "lint": "eslint . --ext .ts,.cjs,.md,.json,.vue",
    "fmt": "eslint . --ext .ts,.cjs,.md,.json,.vue --fix"
  },
  "devDependencies": {
    "@re-taro/eslint-config": "1.6.4",
    "@slidev/cli": "0.40.5",
    "@slidev/types": "0.40.5",
    "@types/node": "18.15.7",
    "eslint": "8.36.0",
    "postcss": "8.4.21",
    "typescript": "5.0.2"
  },
  "slidev": {
    "colorSchema": "light",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "sans": "Inter,Noto Color Emoji",
        "mono": "JetBrains Mono"
      }
    }
  }
}
